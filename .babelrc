{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    [
      "@babel/env",
      {
        "modules": false,
        "loose": true,
        "useBuiltIns": "entry",
        "targets": {
          "browsers": [
            "last 2 Chrome versions",
            "last 2 Firefox versions",
            "last 2 ChromeAndroid versions",
            "last 2 Safari versions",
            "last 2 Edge versions",
            "last 2 iOS versions",
            "ie >= 11",
            "> 5%"
          ]
        }
      }
    ]
  ]
}
